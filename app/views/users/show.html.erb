<p>
  <strong>Username:</strong>
  <%= @user.username %>
</p>
<table>
  <thread>
    <tr>
      <th>Created Events:</th>
    </tr>
  </thread>
  <tbody>
    <% @user.created_events.each do |event| %>
      <tr>
        <td><%= event.event_name %></td>
        <td><%= event.event_date %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<table>
  <thread>
    <tr>
      <th>Attended Events:</th>
    </tr>
  </thread>
  <tbody>
    <% @user.attended_events.each do |event| %>
      <tr>
        <td><%= event.event_name %></td>
        <td><%= event.event_date.join(',') %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to "Home", users_path %>
